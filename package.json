    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
