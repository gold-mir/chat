{
  "name": "webchat",
  "version": "1.0.0",
  "description": "wip",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.{ts,tsx}",
    "build": "npm run buildserver; npm run buildclient",
    "prebuild": "eslint src/**/*.{ts,tsx}",
    "start": "npm run build; node dist/server/server.js",
    "prestart": "eslint src/**/*.{ts,tsx}",
    "buildclient": "rm -rf dist/client; webpack --mode development",
    "prebuildclient": "eslint src/client/**/*.{ts,tsx}",
    "client": "webpack-dev-server",
    "preclient": "eslint src/client/**/*.{ts,tsx}",
    "buildserver": "rm -rf dist/server; tsc -p server.tsconfig.json",
    "prebuildserver": "eslint src/server/**/*.ts",
    "server": "ts-node-dev --respawn --transpileOnly ./src/server/server.ts",
    "preserver": "eslint src/server/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^5.5.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pg-promise": "^9.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  }
}
