{
  "name": "webchat",
  "version": "1.0.0",
  "description": "wip",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.{ts,tsx}",
    "build": "npm run build:server; npm run build:client",
    "prebuild": "eslint src/**/*.{ts,tsx}",
    "start": "npm run build; node dist/server/server.js",
    "prestart": "eslint src/**/*.{ts,tsx}",
    "buildclient": "rm -rf dist/client; webpack --mode development",
    "prebuildclient": "eslint src/client/**/*.{ts,tsx}",
    "startclient": "webpack-dev-server",
    "prestartclient": "eslint src/client/**/*.{ts,tsx}",
    "buildserver": "rm -rf dist/server; tsc -p server.tsconfig.json",
    "prebuildserver": "eslint src/server/**/*.ts",
    "startserver": "ts-node-dev --respawn --transpileOnly ./src/server/server.ts",
    "prestartserver": "eslint src/server/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
