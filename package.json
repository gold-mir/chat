{
  "name": "webchat",
  "version": "1.0.0",
  "description": "wip",
  "main": "index.js",
  "scripts": {
    "build:client": "rm -rf dist/client; webpack --mode development",
    "start:client": "webpack-dev-server",
    "build:server": "rm -rf dist/server; tsc -p server.tsconfig.json",
    "start:server": "ts-node-dev --respawn --transpileOnly ./src/server/server.ts",
    "build": "npm run build:server; npm run build:client",
    "start": "npm run build; node dist/server/server.js"
  },
